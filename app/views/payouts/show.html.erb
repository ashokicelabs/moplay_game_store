<%- model_class = Payout -%>
<div class="page-header">
  <h1><%=t '.title', :default => Payout %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:id) %></dt>
      <dd><pre class="prettyprint"><%= @payout.id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:publisher_id) %></dt>
      <dd><pre class="prettyprint"><%= @payout.publisher_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:amount_paid) %></dt>
      <dd><pre class="prettyprint"><%= @payout.amount_paid %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              payouts_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_payout_path(@payout), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              payout_path(@payout),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

